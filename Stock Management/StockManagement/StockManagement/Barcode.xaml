<Window x:Class="StockManagement.Barcode"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StockManagement"
        mc:Ignorable="d"
        Title="Barcode" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/background-images-10.jpg"/>
        </Grid.Background>
        <Canvas x:Name="_mainFrame" HorizontalAlignment="Left" Height="129" Margin="205,50,0,0" VerticalAlignment="Top" Width="334">
            <Canvas.Background>
                <ImageBrush ImageSource="/77-771409_barcode-png-images-transparent-background-barcode-png-png.png"/>
            </Canvas.Background>
        </Canvas>
        <Button x:Name="btnPrint" Content="Print" HorizontalAlignment="Left" Height="57" Margin="238,327,0,0" VerticalAlignment="Top" Width="268" FontSize="14" FontWeight="Bold" FontFamily="Arial" Click="btnPrint_Click"/>

    </Grid>
</Window>
